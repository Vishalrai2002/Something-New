#include <iostream>
#include<cstring>
using namespace std;

// Rotate String

void Rotate_String(char * a,int n)
{
    int len=strlen(a);
    int i=len-1;
    while(i>=0)
    {
        a[i+n]=a[i];
        i--;
    }
   cout<<a<<endl;


    i=0;
   int j=len;
   
   while(i<n)
   {
       a[i]=a[j];
       i++;
       j++;
    }
    a[len]='\0';
    // cout<<a<<endl;
}

int main() {
   char a[100]="hello";  
   int n=3;
  Rotate_String(a,n);
  cout<<a<<endl;
}
