#include <iostream>
#include<cstring>
using namespace std;

// Count Character frequency


int main() {
  char a[100]="abcbazzz";
  int freq[26]={0};
  // Fill the Frequency Array
  for(int i=0;a[i]!='\0';i++)
  {
      char ch=a[i];
      int idx = ch-'a';
      freq[idx]++;
  }
  for(int i=0;i<26;i++)
  {
      if(freq[i]>0)
      {
          char x=i+'a';
          cout<<"character "<<x<<"->"<<freq[i]<<endl;
      }
  }
}
